
{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "build/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["backend/build/**"] }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "build/index.js"
      }
    ]
  }